{
    "manifest_version": 3,
    "name": "npmjs helper",
    "description": "This extension adds the functionality you need for NPMJS website.",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.npmjs.com/*"
            ],
            "js": [
                "content-scripts/main.tsx"
            ]
        }
    ]
}